---
const { id } = Astro.props;
import Usuarios from '../components/MedicalUsersGrid.astro';


---
<div class="
	w-full 
	dark:bg-[radial-gradient(ellipse_80%_80%_at_50%_-20%,rgba(120,119,198,0.3),rgba(255,255,255,0))]
	dark:[background-size:unset]
	bg-[radial-gradient(#e5e7eb_1px,transparent_1px)] 
	[background-size:16px_16px]
	">
	<section id={id} class="mx-auto min-h-screen max-w-5xl px-4 sm:px-6 lg:px-8 ">
		<Usuarios/>
	</section>
</div>
